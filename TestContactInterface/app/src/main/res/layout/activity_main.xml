<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.xy.testcontactinterface.MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/openDialer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="openDialer"/>

    <Button android:id="@+id/openContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="openContact"/>

    <Button android:id="@+id/openSms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="openSms"/>

</LinearLayout>
